       (sldb-topline-face ((t (:inherit font-lock-warning-face))))
       (slime-repl-input-face ((t (:foreground "maroon"))))
       (slime-repl-output-face ((t (:inherit font-lock-comment-face))))
       (w3m-anchor-face ((((class color) (background light))
(:foreground "SteelBlue4"))))
       (w3m-arrived-anchor-face ((((class color) (background light))
(:foreground "Purple4"))))
