       (gnus-splash-face ((((class color) (background light)) nil)))
       (gnus-summary-normal-read-face ((((class color) (background
light)) (:foreground "aquamarine4"))))
       (highlight ((((class color) (background light)) (:background
"lightsteelblue2"))))
       (minibuffer-prompt ((t (:foreground "steelblue4"))))
       (mode-line ((((type x w32 mac) (class color)) (:background
"grey90" :foreground "grey0" :box (:line-width -1 :color "grey70")))))
       (mode-line-inactive ((t (:inherit mode-line :foreground "grey50"))))
       (sldb-condition-face ((t (:inherit font-lock-type-face))))
